{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "lib": ["ES2020"],
    "resolveJsonModule": true,
    "rootDir": ".",
    "outDir": "./not_actually_used",
    "types": ["probs"]
  },
  "include": [
    "lib/**/*.js",
    "lib/**/*.ts",
    "test/**/*.js",
    "test/**/*.ts",
    "types/typedef.js",
    "types/store.js",
    "./globals.d.ts"
  ],
  "exclude": ["node_modules/**"]
}
